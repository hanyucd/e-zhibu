/* pages/edu/edu.wxss */

page {
  background: #F9F9F9;
}

.banner {
  width: 100%;
  height: 320rpx;
}

.banner navigator {
  font-size: 0;
  position: relative;
  height: 100%;
}

.banner .slide-image {
  width: 100%;
  height: 100%;
}

.grid-edu{
  height: 200rpx;
  width: 100%;
  margin: 1px 0;
  background: #fff;
}

.grid-edu .g-icon{
  width: 84rpx;
  height: 84rpx;
}

.grid-edu .text{
  font-size: 10pt;
  margin-top: 16rpx;
}

.grid-edu-title{
  width: 100%;
  height: 84rpx;
  font-size: 13pt;
  padding: 0 16rpx;
  background: #fff;
}

.study-list .title{
  width: 100%;
  height: 84rpx;
  font-size: 13pt;
  padding: 0 16rpx;
  background: #fff;
  margin-bottom: 1px;
}

.study-list .content{
  padding: 16rpx;
  background: #fff;
  margin-bottom: 1px;
}


.study-list .content .s-icon{
  width: 220rpx;
  height: 220rpx;
  margin-right: 16rpx;
}

.study-list .content .sub-title{
  font-size: 13pt;
  margin-left: 16rpx;
  margin-bottom: 16rpx;
}

.study-list .content .sub-content{
  font-size: 11pt;
  margin-left: 16rpx;
  color: #aaa;
}